	Private Function Replace_Renamed(ByRef InputString_Renamed As String, ByRef SearchString As String, ByRef InsertString As String) As String
		
		'A function to replace as many instances of SearchString with InputString, as are found
		'in InputString.
		'A common usage would be to replace parameters in a localization string
		'Note that wrappers to search for 1,2 and 3 parameters are provided for this function below
		Dim LeftPiece As String
		Dim RightPiece As String
		Dim NewString As String
		NewString = InputString_Renamed
		
		Do While InStr(NewString, SearchString) <> 0
			LeftPiece = Left(NewString, InStr(NewString, SearchString) - 1)
			RightPiece = Right(NewString, Len(NewString) - InStr(NewString, SearchString) - Len(SearchString) + 1)
			NewString = LeftPiece & InsertString & RightPiece
		Loop 
		Replace_Renamed = NewString
	End Function
	
	Public Function txslFormatString1(ByRef InputString_Renamed As String, ByRef InsertString1 As String) As String
		' Function to replace all occurences of %1 in InputString with InsertString1
		txslFormatString1 = Replace_Renamed(InputString_Renamed, "%1", InsertString1)
	End Function
	
	Public Function txslFormatString2(ByRef InputString_Renamed As String, ByRef InsertString1 As String, ByRef InsertString2 As String) As String
		Dim NewString As String
		' Function to replace all occurences of %1 in InputString with InsertString1, plus
		' replace all occureences of %2 in that result with InsertString2
		NewString = Replace_Renamed(InputString_Renamed, "%1", InsertString1)
		txslFormatString2 = Replace_Renamed(NewString, "%2", InsertString2)
	End Function
	
	Public Function txslFormatString3(ByRef InputString_Renamed As String, ByRef InsertString1 As String, ByRef InsertString2 As String, ByRef InsertString3 As String) As String
		' Function to replace all occurences of %1 in InputString with InsertString1, plus
		' replace all occureences of %2 in that result with InsertString2, plus
		' replace all occureences of %3 in that result with InsertString3
		Dim NewString As String
		NewString = Replace_Renamed(InputString_Renamed, "%1", InsertString1)
		NewString = Replace_Renamed(NewString, "%2", InsertString2)
		txslFormatString3 = Replace_Renamed(NewString, "%3", InsertString3)
	End Function
	
	Public Function LangLookup(ByRef StringID As Short) As String
		'a simple function that eliminate the need to type the language parameter
		'when looking up an entry in the gsLangArray table of messages.
		'Given a string ID, it returns the String corresponsing to that id, for the
		'already selected language.
		'If a language string does not exist (indicated by a length of zero), the
		'DefaultLanguage string will be returned
		Dim temp As String
		temp = gsLangArray(StringID, guLanguage)
		If Len(temp) = 0 Then
			temp = gsLangArray(StringID, guDefaultLanguage)
		End If
		LangLookup = temp
		
	End Function
